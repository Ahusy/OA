<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp"
            android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="31dp"
                    android:background="@color/title_color">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="8dp"
                        android:gravity="center_vertical"
                        android:text="报销单"
                        android:textColor="@color/white"
                        android:textSize="15sp"/>

                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="25dp">

                    <TextView
                        android:id="@+id/commit_cclvf"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_30b0ef_5dp"
                        android:gravity="center"
                        android:text="出差旅费报销单"
                        android:textColor="@color/title_color"/>

                    <TextView
                        android:id="@+id/commit_gcxmsp"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="工程项目审批"
                        android:textColor="@color/default_ev_border_color"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="25dp"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="8dp">

                    <TextView
                        android:id="@+id/commit_jksqd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="借款申请单"
                        android:textColor="@color/default_ev_border_color"/>

                    <TextView
                        android:id="@+id/commit_fyzfspd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="费用支付审批单"
                        android:textColor="@color/default_ev_border_color"/>

                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="31dp"
                    android:background="@color/title_color">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="8dp"
                        android:gravity="center_vertical"
                        android:text="审批单"
                        android:textColor="@color/white"
                        android:textSize="15sp"/>

                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="25dp">

                    <TextView
                        android:id="@+id/commit_ccspd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_30b0ef_5dp"
                        android:gravity="center"
                        android:text="出差审批单"
                        android:textColor="@color/title_color"/>

                    <TextView
                        android:id="@+id/commit_ywzdspd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_30b0ef_5dp"
                        android:gravity="center"
                        android:text="业务招待审批单"
                        android:textColor="@color/title_color"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="25dp"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="8dp">

                    <TextView
                        android:id="@+id/commit_htsp"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_30b0ef_5dp"
                        android:gravity="center"
                        android:text="合同审批"
                        android:textColor="@color/title_color"/>

                    <TextView
                        android:id="@+id/commit_gcsysqd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_30b0ef_5dp"
                        android:gravity="center"
                        android:textColor="@color/title_color"
                        android:text="公车使用申请单"/>

                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="2dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="31dp"
                    android:background="@color/title_color">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="8dp"
                        android:gravity="center_vertical"
                        android:text="验收单"
                        android:textColor="@color/white"
                        android:textSize="15sp"/>

                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="25dp">

                    <TextView
                        android:id="@+id/commit_rkd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="入库单"
                        android:textColor="@color/default_ev_border_color"/>

                    <TextView
                        android:id="@+id/commit_ckd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="出库单"
                        android:textColor="@color/default_ev_border_color"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="25dp"
                    android:orientation="horizontal"
                    android:paddingBottom="10dp"
                    android:paddingTop="8dp">

                    <TextView
                        android:id="@+id/commit_gdzcysd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="固定资产验收单"
                        android:textColor="@color/default_ev_border_color"/>

                    <TextView
                        android:id="@+id/commit_dzyhpysd"
                        android:layout_width="0dp"
                        android:layout_height="29dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginStart="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_stroke_5b0ef_5dp_enble"
                        android:gravity="center"
                        android:text="低值易耗验收单"
                        android:textColor="@color/default_ev_border_color"/>

                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>

</ScrollView>
