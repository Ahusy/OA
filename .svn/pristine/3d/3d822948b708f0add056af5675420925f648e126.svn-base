<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="#7e7e7e" android:state_checked="false" />
    <!--选中时的字体颜色  -->
    <item android:color="@color/base_color" android:state_checked="true" />
    <!-- 非触摸模式下获得焦点并单击时的字体颜色 -->
    <item android:color="@color/base_color" android:state_focused="true" android:state_pressed="true" />
</selector>